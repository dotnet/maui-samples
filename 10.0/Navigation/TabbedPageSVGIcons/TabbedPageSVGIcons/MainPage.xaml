<?xml version="1.0" encoding="utf-8"?>
<local:MyTabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    xmlns:local="clr-namespace:TabbedPageSVGIcons"
                    xmlns:android="clr-namespace:Microsoft.Maui.Controls.PlatformConfiguration.AndroidSpecific;assembly=Microsoft.Maui.Controls"
                    x:Class="TabbedPageSVGIcons.MainPage"
                    android:TabbedPage.ToolbarPlacement="Bottom">
    <ContentPage Title="Tab 1"
                 IconImageSource="apple.png">
        <Grid Margin="20">
            <Label Text="Tab 1"
                   HorizontalOptions="Center"
                   VerticalOptions="Center" />
        </Grid>
    </ContentPage>
    <ContentPage Title="Tab 2"
                 IconImageSource="android.png">
        <Grid Margin="20">
            <Label Text="Tab 2"
                   HorizontalOptions="Center"
                   VerticalOptions="Center" />
        </Grid>
    </ContentPage>
    <ContentPage Title="Tab 3"
                 IconImageSource="windows.png">
        <Grid Margin="20">
            <Label Text="Tab 3"
                   HorizontalOptions="Center"
                   VerticalOptions="Center" />
        </Grid>
    </ContentPage>
</local:MyTabbedPage>