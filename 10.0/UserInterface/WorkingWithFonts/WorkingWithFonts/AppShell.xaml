<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WorkingWithFonts.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WorkingWithFonts"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="🖌️ XAML Page">
        <ShellContent Title="🖌️ XAML Page" ContentTemplate="{DataTemplate local:FontPageXaml}" Route="FontPageXaml" />
    </FlyoutItem>
    <FlyoutItem Title="🧑‍💻 C# Page">
        <ShellContent Title="🧑‍💻 C# Page" ContentTemplate="{DataTemplate local:FontPageCs}" Route="FontPageCs" />
    </FlyoutItem>
</Shell>
