<?xml version="1.0" encoding="utf-8" ?>
<nativeEmbedding:App xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                     xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                     xmlns:nativeEmbedding="clr-namespace:NativeEmbeddingDemo;assembly=NativeEmbeddingDemo"
                     x:Class="TestHarnessApp.TestApp">
    <nativeEmbedding:App.Resources>
        <ResourceDictionary>
            <Style x:Key="MainAppOnlyStyle" TargetType="VisualElement" ApplyToDerivedTypes="True">
                <Setter Property="IsVisible" Value="False" />
            </Style>
            <Style x:Key="TestAppOnlyStyle" TargetType="VisualElement" ApplyToDerivedTypes="True">
                <Setter Property="IsVisible" Value="True" />
            </Style>
        </ResourceDictionary>
    </nativeEmbedding:App.Resources>
</nativeEmbedding:App>
