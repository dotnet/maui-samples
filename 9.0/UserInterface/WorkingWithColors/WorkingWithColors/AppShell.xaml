<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WorkingWithColors.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:WorkingWithColors"
    Title="WorkingWithColors">

    <ShellContent
        Title="Home"
        Icon="home.png"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Color Demo"
        Icon="pallete.png"
        ContentTemplate="{DataTemplate local:ColorDemo}"
        Route="ColorDemo" />

    <ShellContent
        Title="Colors in XAML"
        Icon="page.png"
        ContentTemplate="{DataTemplate local:ColorsInXaml}"
        Route="ColorsInXaml" />

</Shell>
